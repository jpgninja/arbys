extends layout


block content
  h1= title

  div.row
    div.col-md-6
      h3 Asks

      table.asks
        thead
          tr
            th Price
            th ETH
            th Total
        tbody
        each val in orderBook.asks
          tr
            td= val[0]
            td= val[1]
            td= val[2]

    div.col-md-6
      h3 Bids

      table.bids
        thead
          tr
            th Price
            th ETH
            th Total
        tbody
        each val in orderBook.bids
          tr
            td= val[0]
            td= val[1]
            td= val[2]